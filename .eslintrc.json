{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": [],
  "extends": ["eslint:recommended", "airbnb"],
  "rules": {
    "comma-dangle": 0,
    "no-unused-vars": "warn",
    "no-unexpected-multiline": "warn",
    "prefer-const": "warn"
  },
  "settings": {},
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "jest": true,
    "es6": true
  }
}
